#!/bin/sh

if [ -f /mnt/conf/etc/rxos_config.json ]
then
    jq "$1=\"$2\"" /mnt/conf/etc/rxos_config.json > /tmp/rxos_config.json && mv /tmp/rxos_config.json /mnt/conf/etc/rxos_config.json
else
    jq "$1=\"$2\"" /etc/rxos_config.json > /tmp/rxos_config.json && mv /tmp/rxos_config.json /mnt/conf/etc/rxos_config.json
fi

